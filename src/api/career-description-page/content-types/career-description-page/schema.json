        "Lightbulb",
        "Arrow",
        "Info"
      ]
    },
    "Content2": {
      "type": "blocks"
    },
    "Icon3": {
      "type": "enumeration",
      "default": "Info",
      "enum": [
        "Lightbulb",
        "Arrow",
        "Info"
      ]
    },
    "Content3": {
      "type": "blocks"
